{
  "name": "cocoapuff",
  "version": "1.0.0",
  "description": "Shimmy, shimmy cocoa puff",
  "scripts": {
    "start": "./scripts/start.sh",
    "dev": "./scripts/dev.sh",
    "test": "./scripts/test.sh",
    "test:watch": "./scripts/testwatch.sh",
    "cover": "./scripts/cover.sh",
    "bundle": "./scripts/bundle.sh",
    "watch": "./scripts/watch.sh",
    "lines": "./scripts/lines.sh",
    "lint": "./scripts/lint.sh",
    "compile": "./scripts/compile.sh",
    "build": "./scripts/build.sh",
    "snippets": "./scripts/snippets.sh",
    "prepush": "./scripts/lint.sh && ./scripts/test.sh",
    "preinstall": "./scripts/preinstall.sh",
    "check": "npm outdated && nsp check && depcheck && retire",
    "dbuild": "npm run bundle && docker build -t cocoapuff .",
    "dup": "docker run --name cocoapuff -d -p 3000:3000 cocoapuff && docker start cocoapuff",
    "ddown": "docker stop cocoapuff && docker rm cocoapuff",
    "dexec": "docker exec -ti cocoapuff /bin/sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/halis/cocoapuff.git"
  },
  "author": "Chris Grimes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halis/cocoapuff/issues"
  },
  "homepage": "https://github.com/halis/cocoapuff",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "depcheck": "0.6.7",
    "enzyme": "2.7.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "mocha": "3.2.0",
    "nock": "9.0.5",
    "node-sass": "^4.8.3",
    "nodemon": "1.11.0",
    "nsp": "2.6.2",
    "react-addons-test-utils": "15.3.2",
    "redux-mock-store": "1.2.2",
    "retire": "1.2.11",
    "sass-lint": "1.10.2",
    "sinon": "1.17.7",
    "webpack": "^1.15.0"
  },
  "dependencies": {
    "boron": "^0.2.3",
    "chalk": "^1.1.3",
    "compression": "^1.7.2",
    "deep-freeze-node": "^1.1.3",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "ramda": "^0.23.0",
    "react": "^15.6.2",
    "react-burger-menu": "^1.12.0",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}

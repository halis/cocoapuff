{
  "name": "cocoapuff",
  "version": "1.0.0",
  "description": "Shimmy, shimmy cocoa puff",
  "scripts": {
    "start": "./scripts/start.sh",
    "dev": "./scripts/dev.sh",
    "test": "./scripts/test.sh",
    "test:watch": "./scripts/testwatch.sh",
    "cover": "./scripts/cover.sh",
    "bundle": "./scripts/bundle.sh",
    "watch": "./scripts/watch.sh",
    "lines": "./scripts/lines.sh",
    "lint": "./scripts/lint.sh",
    "compile": "./scripts/compile.sh",
    "build": "./scripts/build.sh",
    "snippets": "./scripts/snippets.sh",
    "prepush": "./scripts/lint.sh && ./scripts/test.sh",
    "check": "npm outdated && nsp check && depcheck && retire",
    "dbuild": "npm run bundle && docker build -t cocoapuff .",
    "dup": "docker run --name cocoapuff -d -p 3000:3000 cocoapuff && docker start cocoapuff",
    "ddown": "docker stop cocoapuff && docker rm cocoapuff",
    "dexec": "docker exec -ti cocoapuff /bin/sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/halis/cocoapuff.git"
  },
  "author": "Chris Grimes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halis/cocoapuff/issues"
  },
  "homepage": "https://github.com/halis/cocoapuff",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react-app": "^0.2.1",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "depcheck": "0.6.7",
    "enzyme": "2.7.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "mocha": "3.2.0",
    "nock": "9.0.5",
    "node-sass": "^4.2.0",
    "nodemon": "1.11.0",
    "nsp": "2.6.2",
    "react-addons-test-utils": "15.3.2",
    "redux-mock-store": "1.2.2",
    "retire": "1.2.11",
    "sass-lint": "1.10.2",
    "sinon": "1.17.7",
    "webpack": "1.13.2"
  },
  "dependencies": {
    "boron": "0.2.3",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "deep-freeze-node": "1.1.2",
    "es6-promise": "4.0.5",
    "express": "4.14.1",
    "isomorphic-fetch": "2.2.1",
    "ramda": "0.23.0",
    "react": "15.3.2",
    "react-burger-menu": "1.10.10",
    "react-dom": "15.3.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  }
}
